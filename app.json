{
  "expo": {
    "name": "TradeAssist",
    "slug": "tradeassist",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tradeassist",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/app/icon.png",
    "backgroundColor": "#1c1c1c", 
    "ios": {
      "supportsTablet": true
    },
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app/adaptive-icon.png",
        "backgroundColor": "#1c1c1c"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1c1c1c",
          "image": "./assets/app/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
