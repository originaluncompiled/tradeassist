{
  "expo": {
    "name": "TradeAssist",
    "slug": "tradeassist",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tradeassist",
    "userInterfaceStyle": "dark",
    "icon": "./assets/app/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app/adaptive-icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "com.justoriginal.tradeassist"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1c1c1c",
          "image": "./assets/app/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dcafe8f6-89b8-4b02-894c-7e95a1278b56"
      }
    }
  }
}
