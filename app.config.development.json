{
  "expo": {
    "name": "TradeAssist Development",
    "icon": "./assets/app/icon-dev.png",
    "ios": {
      "image": "./assets/icon-dev.png",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app/adaptive-icon-dev.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "tradeassist.dev"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app/splash-icon-dark-dev.png",
          "backgroundColor": "#1c1c1c",
          "imageWidth": 200
        }
      ],
      "expo-sqlite",
      "expo-localization"
    ]
  }
}